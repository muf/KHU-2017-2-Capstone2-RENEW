<!DOCTYPE html>
<html>
  <head>
	<title> Multi-Coords Mapping</title>
  <% include ./common/header.ejs %>
    <!-- Custom CSS -->
    <link rel='stylesheet' href='/css/common/nav-bar.css' />
    <link rel='stylesheet' href='/css/registerPage/registerPage.css' />
    <link rel='stylesheet' href='/css/common/map.css' />
   
     <!--JS -->
    <script src="/javascripts/common/map.js"></script>   
    <script src="/javascripts/common/mapHandlerObject.js"></script>   

  </head>
  <body>  
    <% include ./common/nav-bar.ejs %>

    <div id="main-container">
      <!-- #map-container>#1 -->
      <div id="map-container">
        <div id="map"></div>
      </div>
      <!-- #side-container>#2 -->
      <div id="side-container">
          <div id = "side-sample-container">
            <div>
              <span class = "sample-span">sample1</span><br>
              [{"lat":33.42883,"long":126.8564},{"lat":33.425,"long":126.7558}]
            </div>
            <div>
              <span class = "sample-span">sample2</span><br>
              [{33.4839,126.6202},{33.4839,126.6210}]
            </div>
            <div>
              <span class = "sample-span">sample2</span><br>
              [(37.5983,:126.7975),(33.5004,126.5076)]
