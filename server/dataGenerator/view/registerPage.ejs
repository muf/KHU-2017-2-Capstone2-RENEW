          </div>
          <div id ="side-button-container">
            <button id="add-coords"> +  </button>
            <button id="apply" class="apply"> 적용  </button>
          </div><!--  -->

          <div id ="side-table-container">  
            <table class="table">
              <thead>
                <tr>
                  <th>Num</th>
                  <th>Coords</th>
                  <th>Regex</th>
                </tr>
              </thead>

              <tbody>
              </tbody>

            </table>
          </div><!-- search-container-apply -->
        </div><!--  -->
      </div> <!--  -->
    
     <!-- div id = map 을 찾기 때문에 div map 뒤에 호출해야 한다. -->
